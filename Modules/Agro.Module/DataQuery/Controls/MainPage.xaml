<UserControl x:Class="Agro.Module.DataQuery.MainPage"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:local="clr-namespace:Agro.Module.DataQuery"
                    xmlns:cui="clr-namespace:Agro.LibCore.UI;assembly=Agro.LibCore"
                    mc:Ignorable="d"
                    d:DesignHeight="480"
                    d:DesignWidth="800"
                    >
    <cui:SidebarPage>
        <cui:SidebarPage.LeftPanel>
            <local:NavigationPanelTreeView x:Name="navTree"/>
        </cui:SidebarPage.LeftPanel>
        <local:ContentPanel x:Name="contentPnl"/>
    </cui:SidebarPage>
    <!--<Grid>
        <local:ContentPanel x:Name="contentPnl"/>
        <Border Name="bdrMask" Background="#7F000000" Visibility="Hidden"/>
    </Grid>-->
</UserControl>
