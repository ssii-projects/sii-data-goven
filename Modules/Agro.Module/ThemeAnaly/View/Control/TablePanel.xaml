<UserControl x:Class="Agro.Module.ThemeAnaly.View.Control.TablePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Agro.Module.ThemeAnaly.View.Control"
             xmlns:control="clr-namespace:Agro.Module.ThemeAnaly.View.Control"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <local:TitleBar Grid.Row="0" x:Name="TitleBar"  DataContext="{Binding TitleBarViewModel}" />
        <local:Table Grid.Row="1" DataContext="{Binding TableViewModel}"/>
    </Grid>
</UserControl>
